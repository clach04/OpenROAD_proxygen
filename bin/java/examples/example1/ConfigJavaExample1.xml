        <sub desc='Default value' find='${default_value}' replace='0'/>
        <sub desc='Java PDO type' find='${PDOtype}' replace='FLOAT'/>
        <sub desc='Java class to represent this type' find='${type}' replace='Double'/>
    </type>
    <type name='int'>
        <sub desc='Java PDO uses getXXXX/setXXXX' find='${accesstype}' replace='Integer'/>
        <sub desc='Default value' find='${default_value}' replace='0'/>
