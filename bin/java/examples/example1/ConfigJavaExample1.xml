    </type>
    <type name='string'>
        <sub desc='Java PDO uses getXXXX/setXXXX' find='${accesstype}' replace='String'/>
        <sub desc='Default value' find='${default_value}' replace=''/>
        <sub desc='Java PDO type' find='${PDOtype}' replace='STRING'/>
        <sub desc='Java class to represent this type' find='${type}' replace='String'/>
    </type>
</types>
</root>
