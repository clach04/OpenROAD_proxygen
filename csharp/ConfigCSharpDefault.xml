<root>
<jobs>
<job set="1" inputfile="DTCGen.tpl" outputfile="GeneratedCode\DTC\*U*${class_langname}.cs" />
<job set="3" inputfile="Populator.tpl" outputfile="GeneratedCode\${AppService Name}Populator.cs" />
<job set="3" inputfile="Depopulator.tpl" outputfile="GeneratedCode\${AppService Name}Depopulator.cs" />
<job set="2" inputfile="Facade.tpl" outputfile="GeneratedCode\Facdes\${AppService Name}Facade.cs" />
<job set="4" inputfile="SCPW.tpl" outputfile="GeneratedCode\SCPW\${altscp_name}SCPW.cs" />
</jobs>
