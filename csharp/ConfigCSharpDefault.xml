<types>
<type name="byte">
<sub desc="Language Type" find="${lang_type}" replace="${type}"/>
</type>

<type name="bytearray">
<sub desc="Language Type" find="${lang_type}" replace="${type}"/>
</type>

<type name="datetime">
<sub desc="Language Type" find="${lang_type}" replace="DateTime"/>
<sub desc="Type" find="${type}" replace="DateTime"/>
<sub desc="Default Value" find="${default_value}" replace="new DateTime()"/>
<sub desc="PDO" find="${PDOtype}" replace="DATE"/>
<sub desc="Access" find="${access}" replace="Date"/>
<sub desc="DataType" find="${datatype}" replace="Constants.OR_ASA_DATATYPE_DATE"/>
<sub desc="Default Reference" find="${ref}" replace=""/>
</type>

<type name="decimal">
<sub desc="Language Type" find="${lang_type}" replace="${type}"/>
</type>

<type name="double">
<sub desc="Language Type" find="${lang_type}" replace="${type}"/>
<sub desc="Default" find="${default_value}" replace="0.0"/>
<sub desc="Access" find="${access}" replace="Double"/>
<sub desc="DataType" find="${datatype}" replace="Constants.OR_ASA_DATATYPE_FLOAT"/>
<sub desc="Default Reference" find="${ref}" replace=""/>
</type>

<type name="float">
<sub desc="Language Type" find="${lang_type}" replace="${type}"/>
<sub desc="Default Value" find="${default_value}" replace="0.0"/>
<sub desc="PDO" find="${PDOtype}" replace="FLOAT"/>
<sub desc="Access" find="${access}" replace="Double"/>
<sub desc="Datatype" find="${datatype}" replace="Constants.OR_ASA_DATATYPE_FLOAT"/>
<sub desc="Default Reference" find="${ref}" replace=""/>
</type>

<type name="int">
<sub desc="Language Type" find="${lang_type}" replace="${type}"/>
<sub desc="Default Value" find="${default_value}" replace="0"/>
<sub desc="Access" find="${access}" replace="Integer"/>
<sub desc="Datatype" find="${datatype}" replace="Constants.OR_ASA_DATATYPE_INTEGER"/>
<sub desc="Default Reference" find="${ref}" replace=""/>
</type>

<type name="money">
<sub desc="Language Type" find="${lang_type}" replace="${type}"/>
<sub desc="Type" find="${type}" replace="decimal"/>
<sub desc="Default Value" find="${default_value}" replace="0"/>
<sub desc="PDOType" find="${PDOtype}" replace="MONEY"/>
<sub desc="Datatype" find="${datatype}" replace="Constants.OR_ASA_DATATYPE_MONEY"/>
<sub desc="Access" find="${access}" replace="Decimal"/>
<sub desc="Default Reference" find="${ref}" replace=""/>
</type>

<type name="short">
<sub desc="Language Type" find="${lang_type}" replace="${type}"/>
<sub desc="Default Value" find="${default_value}" replace="0"/>
